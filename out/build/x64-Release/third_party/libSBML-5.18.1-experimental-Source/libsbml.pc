Name: libSBML
Description: A library for reading/writing/manipulating SBML
URL: http://sbml.org/Software/libSBML
Version: 5.18.1

prefix=D:/rrbuild/roadrunner/out/install/x64-Release
libdir=D:/rrbuild/roadrunner/out/install/x64-Release/lib
includedir=D:/rrbuild/roadrunner/out/install/x64-Release/include

Libs: -L${libdir} -lsbml
Libs.private:        $<TARGET_LINKER_FILE:LibXml2> D:/vcpkg/installed/x64-windows/lib/bz2.lib D:/CppLibraries/libsbml-without-namespace/lib/zdll.lib -lstdc++ -lm
Cflags: -I${includedir}
Requires.private: libxml-2.0 >= 2.6.22

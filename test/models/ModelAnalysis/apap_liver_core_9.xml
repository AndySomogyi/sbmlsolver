    <listOfRules>
      <assignmentRule variable="necrosis">
        <math xmlns="http://www.w3.org/1998/Math/MathML" xmlns:sbml="http://www.sbml.org/sbml/level3/version1/core">
          <piecewise>
            <piece>
              <cn sbml:units="dimensionless" type="integer"> 1 </cn>
              <apply>
                <gt/>
                <ci> napqi </ci>
                <ci> necrosis_threshold </ci>
              </apply>
            </piece>
            <otherwise>
              <cn sbml:units="dimensionless" type="integer"> 0 </cn>
            </otherwise>
          </piecewise>
        </math>
      </assignmentRule>
      <assignmentRule variable="d_apap_ext__dt">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <minus/>
            <ci> APAPIM </ci>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule variable="d_vapap_ext__d_apap_ext">
        <math xmlns="http://www.w3.org/1998/Math/MathML" xmlns:sbml="http://www.sbml.org/sbml/level3/version1/core">
          <apply>
            <divide/>
            <apply>
              <times/>
              <apply>
                <divide/>
                <apply>
                  <times/>
                  <apply>
                    <minus/>
                    <cn sbml:units="dimensionless" type="integer"> 1 </cn>
                    <ci> necrosis </ci>
                  </apply>
                  <ci> APAPIM_Vmax </ci>
                </apply>
                <ci> APAPIM_Km_apap </ci>
              </apply>
              <ci> Vli </ci>
            </apply>
            <apply>
              <plus/>
              <cn sbml:units="dimensionless" type="integer"> 1 </cn>
              <apply>
                <divide/>
                <ci> apap_ext </ci>
                <ci> APAPIM_Km_apap </ci>
              </apply>

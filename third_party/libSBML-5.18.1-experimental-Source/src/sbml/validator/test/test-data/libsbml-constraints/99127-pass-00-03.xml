<!--
Pass: (1604) A KineticLaw's substanceUnits must be 'substance',
'item', 'mole', or the id of a UnitDefinition that defines a variant
of 'item' or 'mole'.
 -->

<sbml xmlns="http://www.sbml.org/sbml/level2" level="2" version="1">
  <model>
    <listOfCompartments>
      <compartment id="c"/>
    </listOfCompartments>
    <listOfSpecies>
      <species id="s" compartment="c" initialAmount="0"/>
    </listOfSpecies>
    <listOfParameters>
      <parameter id="t" value="0" units="second"/>
    </listOfParameters>
    <listOfReactions>
      <reaction id="r">
        <listOfReactants>
          <speciesReference species="s"/>
        </listOfReactants>
        <kineticLaw substanceUnits="mole">          
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <divide/>
              <apply>
                <times/>
                <ci> s </ci>
                <ci> c </ci>
              </apply>
              <ci> t </ci>
            </apply>
          </math>          
        </kineticLaw>
      </reaction>
    </listOfReactions>
  </model>
</sbml>

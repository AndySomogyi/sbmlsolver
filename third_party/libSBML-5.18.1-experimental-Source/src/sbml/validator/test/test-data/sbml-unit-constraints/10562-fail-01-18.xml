<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level3/version1/core" level="3" version="1">
  <model timeUnits="time">
    <listOfUnitDefinitions>
      <unitDefinition id="time">
        <listOfUnits>
          <unit kind="second" exponent="1" scale="0" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
    </listOfUnitDefinitions>
    <listOfCompartments>
      <compartment id="c" spatialDimensions="3" units="litre" constant="false"/>
    </listOfCompartments>
    <listOfSpecies>
      <species id="s1" compartment="c" initialConcentration="0" substanceUnits="mole" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
    </listOfSpecies>
    <listOfParameters>
      <parameter id="p" value="1" units="litre" constant="false"/>
      <parameter id="p1" value="1" units="ampere" constant="false"/>
      <parameter id="dummy_p_1" value="1" units="dimensionless" constant="true"/>
      <parameter id="dummy_p_2" value="1" units="dimensionless" constant="true"/>
    </listOfParameters>
    <listOfEvents>
      <event id="e" useValuesFromTriggerTime="true">
        <trigger initialValue="true" persistent="true">
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <gt/>
              <ci> dummy_p_1 </ci>
              <ci> dummy_p_2 </ci>
            </apply>
          </math>
        </trigger>
        <listOfEventAssignments>
          <eventAssignment variable="s1">
            <math xmlns="http://www.w3.org/1998/Math/MathML">
              <apply>
                <divide/>
                <ci> p1 </ci>
                <ci> p </ci>
              </apply>
            </math>
          </eventAssignment>
        </listOfEventAssignments>
      </event>
    </listOfEvents>
  </model>
</sbml>
